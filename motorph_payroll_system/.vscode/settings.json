{    "java.configuration.updateBuildConfiguration": "automatic",
    "java.compile.nullAnalysis.mode": "automatic",
    "java.debug.settings.enableRunDebugCodeLens": true,
    "java.debug.settings.showHex": false,
    "java.debug.settings.showStaticVariables": true,
    "java.debug.settings.showQualifiedNames": false,
    "java.debug.settings.maxStringLength": 0,
    "java.debug.settings.numericPrecision": 0,
    "java.debug.settings.hotCodeReplace": "auto",
    "java.debug.settings.enableHotCodeReplace": true,
    "java.debug.settings.stepping.skipClasses": [],
    "java.debug.settings.stepping.skipSynthetics": true,
    "java.debug.settings.stepping.skipStaticInitializers": true,
    "java.debug.settings.stepping.skipConstructors": false,
    "maven.terminal.useJavaHome": true,
    "maven.executable.path": "mvn",
    "java.import.maven.enabled": true,
    "java.saveActions.organizeImports": true,
    "java.cleanup.actions": [
        "addOverride",
        "addDeprecated",
        "stringConcatToTextBlock",
        "invertEquals",
        "addFinalModifier",
        "instanceofPatternMatch",
        "lambdaExpression",
        "switchExpression"
    ],
    "java.configuration.runtimes": [],
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx4G -Xms100m",
    "java.import.gradle.enabled": false,
    "java.project.sourcePaths": ["src/main/java"],
    "java.project.outputPath": "target/classes",
    "java.project.referencedLibraries": ["target/dependency/*.jar"],
    "java.server.launchMode": "Standard",
    "java.sources.organizeImports.starThreshold": 99,
    "java.sources.organizeImports.staticStarThreshold": 99,
    "java.eclipse.downloadSources": true,
    "java.maven.downloadSources": true,
    "java.referencesCodeLens.enabled": true,
    "java.implementationsCodeLens.enabled": true,
    "java.signatureHelp.enabled": true,
    "java.contentProvider.preferred": "fernflower",
    "java.format.settings.url": ".vscode/java-formatter.xml",
    "java.format.settings.profile": "GoogleStyle",
    "java.codeGeneration.insertionLocation": "afterCursor",
    "java.codeGeneration.generateComments": true,
    "java.completion.favoriteStaticMembers": [
        "org.junit.Assert.*",
        "org.junit.Assume.*",
        "org.junit.jupiter.api.Assertions.*",
        "org.junit.jupiter.api.Assumptions.*",
        "org.junit.jupiter.api.DynamicContainer.*",
        "org.junit.jupiter.api.DynamicTest.*",
        "org.mockito.Mockito.*",
        "org.mockito.ArgumentMatchers.*",
        "org.mockito.Answers.*"
    ],
    "java.maxConcurrentBuilds": 1,
    "java.autobuild.enabled": true,
    "java.errors.incompleteClasspath.severity": "warning",
    "java.test.config": {
        "workingDirectory": "${workspaceFolder}",
        "vmArgs": ["-Xmx1024m", "-Dfile.encoding=UTF-8"]
    },
    "files.exclude": {
        "**/target/**": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    }
}
